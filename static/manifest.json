{
  "manifest_version": 2,

  "name": "RBFT",
  "description": "This extension lets you connect in real-time to see what's making your friends happy!",
  "version": "0.1",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
